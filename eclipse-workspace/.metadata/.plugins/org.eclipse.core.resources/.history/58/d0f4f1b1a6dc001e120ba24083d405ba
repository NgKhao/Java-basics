package view;

import javax.swing.JFrame;

public class ViDu {
	public static void main(String[] args) {
		JFrame jf = new JFrame();
		jf.setTitle("Vi Du JFrame");
		jf.setSize(600, 400);
		// mở cửa sổ nếu true
		jf.setVisible(true);
		
		while(true) {
			System.out.println("CHuong trinh dang chay");
		}
	}
}
